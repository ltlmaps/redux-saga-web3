{
  "name": "redux-saga-web3",
  "version": "0.1.0-alpha.2",
  "main": "lib/index.js",
  "dependencies": {
    "immutable": "4.0.0-rc.12",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-saga-web3-eth-subscribe": "^0.1.0-alpha.0",
    "reselect": "^4.0.0",
    "web3": "1.0.0-beta.55",
    "web3-eth": "1.0.0-beta.55"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "run-when-changed": "^2.1.0"
  },
  "scripts": {
    "build": "babel src/ --out-dir lib/",
    "clean": "rimraf lib/",
    "dev": "run-when-changed --watch 'src/**/*.js' --exec 'npm run prepare'",
    "prepare": "npm run clean && npm run build",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.js\"",
    "link-deps": "npm link redux-saga-web3-eth-subscribe"
  }
}
