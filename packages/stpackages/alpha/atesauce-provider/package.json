{
  "name": "statesauce-provider",
  "version": "0.1.0",
  "description": "Provider react component for statesauce store",
  "main": "src/index.js",
  "author": "Seth Feibus <sethfork@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src/ --out-dir lib/",
    "clean": "rimraf lib/",
    "dev": "run-when-changed --watch 'src/**/*.js' --exec 'yarn run prepublish'",
    "prepare": "yarn run clean && yarn run build",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-redux": "^5.0.7",
    "statesauce-constants": "^0.1.0",
    "statesauce-store": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-watch": "^2.0.7",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "prettier": "^1.13.3",
    "rimraf": "^2.6.2",
    "run-when-changed": "^2.1.0",
    "standard": "^11.0.1"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sethfork/statesauce/packages/statesauce-provider"
  },
  "keywords": [
    "redux",
    "web3",
    "ethereum",
    "provider",
    "statesauce"
  ]
}
